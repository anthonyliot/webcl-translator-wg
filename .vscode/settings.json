{
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "cmakeFormat.args": [
        "--config-file",
        "${workspaceRoot}/.cmake-format.json"
    ],
    "clang-format.style": "Microsoft",
    "pre-commit-helper.runOnSave": "all hooks",
    "pre-commit-helper.config": "${workspaceRoot}/.pre-commit-config.yaml",
    "files.associations": {
        "vector": "cpp",
        "__config": "cpp",
        "__verbose_abort": "cpp",
        "cfenv": "cpp",
        "cmath": "cpp",
        "complex": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "cwchar": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "optional": "cpp",
        "stdexcept": "cpp",
        "tuple": "cpp",
        "typeinfo": "cpp",
        "algorithm": "cpp",
        "memory": "cpp",
        "__bit_reference": "cpp",
        "bitset": "cpp",
        "map": "cpp",
        "set": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "__locale": "cpp",
        "array": "cpp",
        "span": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "chrono": "cpp",
        "__hash_table": "cpp",
        "__split_buffer": "cpp",
        "__tree": "cpp",
        "deque": "cpp",
        "list": "cpp",
        "queue": "cpp",
        "regex": "cpp",
        "stack": "cpp",
        "valarray": "cpp",
        "*.inc": "cpp",
        "__threading_support": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cstdarg": "cpp",
        "ctime": "cpp",
        "cwctype": "cpp",
        "execution": "cpp",
        "ios": "cpp",
        "locale": "cpp",
        "mutex": "cpp",
        "ostream": "cpp",
        "print": "cpp",
        "ratio": "cpp",
        "streambuf": "cpp",
        "variant": "cpp",
        "*.resources": "cpp"
    },
}
